                                                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#used"/>
                                                <owl:someValuesFrom>
                                                    <owl:Class>
                                                        <owl:intersectionOf rdf:parseType="Collection">
                                                            <owl:Class>
                                                                <owl:unionOf rdf:parseType="Collection">
