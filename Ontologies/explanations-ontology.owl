                                                                <owl:someValuesFrom rdf:resource="https://purl.org/heals/eo#SystemTrace"/>
                                                            </owl:Restriction>
                                                        </owl:intersectionOf>
                                                    </owl:Class>
                                                </owl:someValuesFrom>
                                            </owl:Restriction>
                                        </owl:intersectionOf>
                                    </owl:Class>
