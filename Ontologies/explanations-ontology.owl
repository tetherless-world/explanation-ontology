    <!-- http://linkedu.eu/dedalo/explanationPattern.owl#Explanation -->

    <rdf:Description rdf:about="http://linkedu.eu/dedalo/explanationPattern.owl#Explanation">
        <rdfs:subClassOf rdf:resource="http://semanticscience.org/resource/SIO_000602"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://linkedu.eu/dedalo/explanationPattern.owl#hasExplanans"/>
