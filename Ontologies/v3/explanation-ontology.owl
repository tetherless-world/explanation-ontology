                                                        <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#used"/>
                                                        <owl:someValuesFrom rdf:resource="https://purl.org/heals/eo#ExplanationMethod"/>
                                                    </owl:Restriction>
                                                </owl:intersectionOf>
                                            </owl:Class>
                                        </owl:someValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://linkedu.eu/dedalo/explanationPattern.owl#isBasedOn"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="https://purl.org/heals/eo#SystemRecommendation"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
                                        <owl:someValuesFrom>
                                            <owl:Class>
                                                <owl:intersectionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="https://purl.org/heals/eo#AITask"/>
                                                    <owl:Restriction>
                                                        <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#used"/>
