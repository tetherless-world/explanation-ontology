        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/heals/eo#hasExplanationType"/>
                <owl:someValuesFrom rdf:resource="https://purl.org/heals/eo#InteractiveExplanationType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:label>Interactive Explanation</rdfs:label>
    </owl:Class>
    


