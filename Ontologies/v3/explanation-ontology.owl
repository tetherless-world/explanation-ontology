        <rdfs:subClassOf rdf:resource="https://purl.org/heals/eo#ExplanationType"/>
        <rdfs:label>Type - Interactive Explanation</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/heals/eo#InterprationAndExplanationTask -->

    <owl:Class rdf:about="https://purl.org/heals/eo#InterprationAndExplanationTask">
        <rdfs:subClassOf rdf:resource="https://purl.org/heals/eo#AITask"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#used"/>
                <owl:someValuesFrom rdf:resource="https://purl.org/heals/eo#ExplanationMethod"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <dc:description>a task that produces interpretations or explanations, which are an account of the system, its workings, the implicit and explicit knowledge it uses to arrive at conclusions in general and the specific decision at hand, that is sensitive to the end-user’s understanding, context, and current needs.</dc:description>
        <terms:source>Chari, S., Gruen, D., Seneviratne, O.W., &amp; McGuinness, D.L. (2020). Directions for Explainable Knowledge-Enabled Systems. Knowledge Graphs for eXplainable Artificial Intelligence.</terms:source>
        <rdfs:label>Interpretation and Explanation Task</rdfs:label>
    </owl:Class>
    


