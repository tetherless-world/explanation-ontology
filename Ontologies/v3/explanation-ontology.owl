                                                            <owl:Class>
                                                                <owl:intersectionOf rdf:parseType="Collection">
                                                                    <rdf:Description rdf:about="https://purl.org/heals/eo#ArtificialIntelligenceMethod"/>
                                                                    <owl:Restriction>
                                                                        <owl:onProperty rdf:resource="http://semanticscience.org/resource/SIO_000229"/>
                                                                        <owl:someValuesFrom rdf:resource="https://purl.org/heals/eo#SystemTrace"/>
                                                                    </owl:Restriction>
