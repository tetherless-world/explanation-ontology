        <owl:equivalentClass>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://linkedu.eu/dedalo/explanationPattern.owl#isBasedOn"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:intersectionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://purl.org/heals/eo#ModelExplanationOutput"/>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://semanticscience.org/resource/SIO_000232"/>
