        <rdfs:label>Knowledge based Systems</rdfs:label>
    </owl:Class>
    


    <!-- https://purl.org/heals/eo#KnowledgeDistillationMethod -->

    <owl:Class rdf:about="https://purl.org/heals/eo#KnowledgeDistillationMethod">
