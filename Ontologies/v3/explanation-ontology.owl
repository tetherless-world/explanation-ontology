                                                                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#wasGeneratedBy"/>
                                                                <owl:someValuesFrom rdf:resource="https://purl.org/heals/eo#ScientificMethod"/>
                                                            </owl:Restriction>
                                                        </owl:unionOf>
                                                    </owl:Class>
